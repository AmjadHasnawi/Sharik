    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "cookie-session": "^2.0.0-beta.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.6",
    "faker": "^4.1.0",
    "jquery": "^3.3.1",
    "mocha": "^5.2.0",
    "mongoose": "^5.4.0",
    "mysql": "^2.16.0",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "stripe": "^6.20.0",
    "supertest": "^3.3.0",
    "twilio": "^3.26.0"
  },
